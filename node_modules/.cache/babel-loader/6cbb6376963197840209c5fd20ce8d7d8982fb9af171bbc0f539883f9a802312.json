{"ast":null,"code":"import Fullbanner from '@/components/home/fullbanner.vue';\nexport default {\n  name: 'Home',\n  components: {\n    Fullbanner\n  }\n};","map":{"version":3,"names":["Fullbanner","name","components"],"sources":["/Users/vagnerzezo/Documents/development/project/ecommerce/src/views/Home.vue"],"sourcesContent":["<template>\n  <div>\n    <Fullbanner />\n    <template>\n  <div>\n    <!-- Exibindo a lista de produtos -->\n    <div v-if=\"allProducts.length > 0\">\n      <h2>Produtos</h2>\n      <ul>\n        <li v-for=\"product in allProducts\" :key=\"product.id\">\n          {{ product.title }} - ${{ product.price }}\n          <button @click=\"adicionarAoCarrinho(product)\">Adicionar ao carrinho</button>\n        </li>\n      </ul>\n    </div>\n    <div v-else>\n      <p>Carregando produtos...</p>\n    </div>\n\n    <!-- Exibindo o carrinho -->\n    <div v-if=\"cartProducts.length > 0\">\n      <h2>Carrinho</h2>\n      <ul>\n        <li v-for=\"product in cartProducts\" :key=\"product.id\">\n          {{ product.title }} - ${{ product.price }}\n          <button @click=\"removerDoCarrinho(product.id)\">Remover do carrinho</button>\n        </li>\n      </ul>\n      <button @click=\"limparCarrinho\">Limpar Carrinho</button>\n    </div>\n    <div v-else>\n      <p>O carrinho está vazio.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'SeuComponente',\n  computed: {\n    allProducts() {\n      return this.$store.getters.allProducts;\n    },\n    cartProducts() {\n      return this.$store.getters.cartProducts;\n    }\n  },\n  methods: {\n    adicionarAoCarrinho(product) {\n      this.$store.dispatch('addProductToCart', product);\n    },\n    removerDoCarrinho(productId) {\n      this.$store.dispatch('removeProductFromCart', productId);\n    },\n    limparCarrinho() {\n      this.$store.dispatch('clearCart');\n    }\n  },\n  mounted() {\n    // Chamando a action fetchProducts quando o componente é montado\n    this.$store.dispatch('fetchProducts');\n  }\n}\n</script>\n\n  </div>\n</template>\n<script>\nimport Fullbanner from '@/components/home/fullbanner.vue'\n\n\nexport default {\n  name: 'Home',\n  components: {\n    Fullbanner\n  }\n}\n</script>\n"],"mappings":"AAoEA,OAAOA,UAAS,MAAO,kCAAiC;AAGxD,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IACVF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}